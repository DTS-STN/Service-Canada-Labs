    ],
    "components/**/*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write"
    ],
    "public/locales/**/*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write"
    ],
    "middlewares/**/*.{js,jsx,ts,tsx,json,md}": [
